// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;

// Kinematic viscosity - represents the molecular viscosity in LES
// LES models will add the subgrid-scale viscosity automatically
nu              nu [ 0 2 -1 0 0 0 0 ] 1e-06;


